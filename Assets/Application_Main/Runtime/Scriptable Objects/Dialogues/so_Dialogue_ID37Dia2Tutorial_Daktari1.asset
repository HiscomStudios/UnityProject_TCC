MonoBehaviour:
  m_ObjectHideFlags: 0
  m_CorrespondingSourceObject: {fileID: 0}
  m_PrefabInstance: {fileID: 0}
  m_PrefabAsset: {fileID: 0}
  m_GameObject: {fileID: 0}
  m_Enabled: 1
  m_EditorHideFlags: 0
  m_Script: {fileID: 11500000, guid: 524271193aa441aa84eba3efd64f23d5, type: 3}
  m_Name: so_Dialogue_ID37Dia2Tutorial_Daktari1
  m_EditorClassIdentifier: 
  dialogueName: so_Dialogue_ID37Dia2Tutorial_Daktari1
  dialogueId: 37
  dialogueContent:
  - speakerId: character_Niara
    sentence: "Bom dia, anci\xE3o. Como est\xE1?"
    icon: {fileID: 21300000, guid: 721763a55c032a845b25359d34d83369, type: 3}
    audio: {fileID: 0}
  - speakerId: character_Niara
    sentence: "Minha m\xE3e falou que o senhor queria falar comigo."
    icon: {fileID: 21300000, guid: 721763a55c032a845b25359d34d83369, type: 3}
    audio: {fileID: 0}
  - speakerId: character_Daktari
    sentence: "Bom dia Niara, estou bem sim e voc\xEA?"
    icon: {fileID: 21300000, guid: 8efbc480da1a32f428184f4acc60120d, type: 3}
    audio: {fileID: 0}
  - speakerId: character_Daktari
    sentence: "N\xE3o era nada demais o que eu queria falar com voc\xEA, apenas te
      parabenizar pelo seu progresso com o treinamento de guerreira."
    icon: {fileID: 21300000, guid: 8efbc480da1a32f428184f4acc60120d, type: 3}
    audio: {fileID: 0}
  - speakerId: character_Daktari
    sentence: "Luena me contou que voc\xEA tem melhorado cada vez mais."
    icon: {fileID: 21300000, guid: 8efbc480da1a32f428184f4acc60120d, type: 3}
    audio: {fileID: 0}
  - speakerId: character_Niara
    sentence: "Fico feliz em saber que a general Luena ache que estou progredindo,
      mesmo que n\xE3o tenhamos treinado t\xE9cnicas de ataque e luta at\xE9 agora."
    icon: {fileID: 21300000, guid: 721763a55c032a845b25359d34d83369, type: 3}
    audio: {fileID: 0}
  - speakerId: character_Daktari
    sentence: "Ora crian\xE7a, um guerreiro n\xE3o \xE9 s\xF3 aquele que batalha
      em guerras ou tem o melhor ataque."
    icon: {fileID: 21300000, guid: 8efbc480da1a32f428184f4acc60120d, type: 3}
    audio: {fileID: 0}
  - speakerId: character_Daktari
    sentence: "Ser uma guerreira \xE9, conseguir proteger aqueles que n\xE3o podem
      fazer isso sozinhos, mesmo aqueles que est\xE3o para al\xE9m dos dom\xEDnios
      m\xE1gicos de Zile Beni e da Grande M\xE3e."
    icon: {fileID: 21300000, guid: 8efbc480da1a32f428184f4acc60120d, type: 3}
    audio: {fileID: 0}
  - speakerId: character_Niara
    sentence: O senhor quer dizer, como o meu pai fazia?
    icon: {fileID: 21300000, guid: 721763a55c032a845b25359d34d83369, type: 3}
    audio: {fileID: 0}
  - speakerId: character_Daktari
    sentence: "Exatamente como o seu pai, ele era um dos nossos melhores guerreiros
      e salvou muita gente da escravid\xE3o."
    icon: {fileID: 21300000, guid: 8efbc480da1a32f428184f4acc60120d, type: 3}
    audio: {fileID: 0}
  - speakerId: character_Daktari
    sentence: "Ele sempre acreditou em voc\xEA e no seu potencial, Niara."
    icon: {fileID: 21300000, guid: 8efbc480da1a32f428184f4acc60120d, type: 3}
    audio: {fileID: 0}
  - speakerId: character_Niara
    sentence: Obrigada, senhor Daktari, saber dessas coisas faz a saudade diminuir
      um pouco.
    icon: {fileID: 21300000, guid: 721763a55c032a845b25359d34d83369, type: 3}
    audio: {fileID: 0}
  - speakerId: character_Niara
    sentence: "Todo dia eu agrade\xE7o por ter tido bons momentos com ele, e por
      ainda ter minha m\xE3e comigo."
    icon: {fileID: 21300000, guid: 721763a55c032a845b25359d34d83369, type: 3}
    audio: {fileID: 0}
  - speakerId: character_Daktari
    sentence: "E por falar na sua m\xE3e\u2026 Lembra quando voc\xEA pediu para a
      Grande M\xE3e que ela lhe trouxesse de volta o cajado aben\xE7oado que seu
      pai usava nas buscas?"
    icon: {fileID: 21300000, guid: 8efbc480da1a32f428184f4acc60120d, type: 3}
    audio: {fileID: 0}
  - speakerId: character_Niara
    sentence: "Uhum, mas o que isso tem haver com a minha m\xE3e?"
    icon: {fileID: 21300000, guid: 721763a55c032a845b25359d34d83369, type: 3}
    audio: {fileID: 0}
  - speakerId: character_Daktari
    sentence: "Depois que o seu pedido foi recusado, Amai foi ao templo todos os
      dias orar e suplicar para que a Grande M\xE3e fosse misericordiosa e atendesse
      o seu pedido."
    icon: {fileID: 21300000, guid: 8efbc480da1a32f428184f4acc60120d, type: 3}
    audio: {fileID: 0}
  - speakerId: character_Daktari
    sentence: "Ela at\xE9 mesmo pediu para que eu usasse minha proximidade com a
      Grande M\xE3e e pedisse diretamente para ela."
    icon: {fileID: 21300000, guid: 8efbc480da1a32f428184f4acc60120d, type: 3}
    audio: {fileID: 0}
  - speakerId: character_Daktari
    sentence: E uma coisa maravilhosa aconteceu.
    icon: {fileID: 21300000, guid: 8efbc480da1a32f428184f4acc60120d, type: 3}
    audio: {fileID: 0}
  - speakerId: character_Niara
    sentence: 'O que aconteceu? '
    icon: {fileID: 21300000, guid: 721763a55c032a845b25359d34d83369, type: 3}
    audio: {fileID: 0}
  - speakerId: character_Daktari
    sentence: "A Grande M\xE3e veio falar comigo, e disse que depois de muito ouvir
      o desejo sincero de uma m\xE3e que ama muito sua filha\u2026"
    icon: {fileID: 21300000, guid: 8efbc480da1a32f428184f4acc60120d, type: 3}
    audio: {fileID: 0}
  - speakerId: character_Daktari
    sentence: "Ela resolveu atender o seu pedido e me entregou o antigo cajado de
      seu pai, ela at\xE9 mesmo aben\xE7oou ele de novo para voc\xEA us\xE1-lo."
    icon: {fileID: 21300000, guid: 7ceced71a7c3a3c41baad70460dadace, type: 3}
    audio: {fileID: 0}
